  "scripts": {
    "build": "nest build",
    "heroku-postbuild": "npm run build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch --trace-deprecation",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "worker": "node dist/worker/main.js",
    "worker:dev": "ts-node -r tsconfig-paths/register src/worker/main.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "email:preview": "tsx src/infra/services/mail/preview/server.tsx",
    "pdf:preview": "tsx src/shared/utils/pdfs/preview/server.tsx"
  },
